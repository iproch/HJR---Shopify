                #header.white .user .account a:hover {
                    color: #000000;
                }

/* header search, logo and cart */
#header .search-logo-cart {
    position: relative;
    height: 128px;
    border-top: 1px solid #222222;
    border-bottom: 1px solid #222222;
}

    #header.white .search-logo-cart {
        border-color: #f1f1f1;
    }

    #header .search-logo-cart .container {
        position: relative;
        height: 100%;
    }

    /* header logo */
    #header .search-logo-cart h1 {
        position: absolute;
        top: 0;
        right: 0;
        bottom: 0;
        left: 0;
        margin: auto;
        width: 369px;
        height: 75px;
    }

        #header .search-logo-cart h1 a {
            display: block;
            height: 75px;
            background: transparent
                        url(h1-haus-of-jr-logo-white.png) center center no-repeat;
            background-size: 369px 75px;
            text-indent: -99999px;
        }

            #header.white .search-logo-cart h1 a {
                background: transparent
                            url(h1-haus-of-jr-logo-black.png) center center no-repeat;
                background-size: 369px 75px;
            }

    /* header search */
    #header .search {
        position: absolute;
        top: 0;
        bottom: 0;
        left: 0;
        margin: auto;
        height: 40px;
        border: 1px solid #222222;
        -moz-border-radius: 3px;
        -webkit-border-radius: 3px;
        border-radius: 3px;
    }

        #header.white .search {
            border-color: #f1f1f1;
        }

        #header .search fieldset {
            position: relative;
        }

            #header .search input[type='search'] {
                float: left;
                padding: 0 36px 0 10px;
                width: 200px;
                height: 40px;
                font-family: 'Montserrat', sans-serif;
                font-size: 12px;
                background: transparent;
                border: 0;
                color: #848383;
                line-height: 1;
            }

                #header.white .search input[type='search'] {
                    color: #000000;
                }

                #header .search input[type='search']::-moz-placeholder {
                    color: #959595;
                }

                    #header.white .search input[type='search']::-moz-placeholder {
                        color: #6b6b6b;
                    }

                #header .search input[type='search']::-ms-input-placeholder {
                    color: #959595;
                }

                    #header.white .search input[type='search']::-ms-input-placeholder {
                        color: #6b6b6b;
                    }

                #header .search input[type='search']::-webkit-input-placeholder {
                    color: #959595;
                }

                    #header.white .search input[type='search']::-webkit-input-placeholder {
                        color: #6b6b6b;
                    }

                #header .search input[type='search']:focus {
                    background: #222222;
                }

                    #header.white .search input[type='search']:focus {
                        background: #f1f1f1;
                    }

            #header .search button {
                position: absolute;
                bottom: 0;
                right: 0;
                width: 36px;
                height: 40px;
                background: transparent;
            }

                #header .search button span {}

                    #header .search button span:before {
                        font-family: 'FontAwesome';
                        font-size: 16px;
                        color: #959595;
                    }

                        #header .search button:hover span:before {
                            color: #ffffff;
                        }

                            #header.white .search button:hover span:before {
                                color: #000000;
                            }

    /* header cart */
    #header .search-logo-cart .fa-shopping-cart {
        position: absolute;
        top: 0;
        right: 0;
        bottom: 0;
        margin: auto;
        padding: 0 20px;
        height: 40px;
        border: 1px solid #222222;
        -moz-border-radius: 3px;
        -webkit-border-radius: 3px;
        border-radius: 3px;
        font-size: 10px;
        color: #cccccc;
        line-height: 40px;
    }

        #header.white .search-logo-cart .fa-shopping-cart {
            border-color: #f1f1f1;
            color: #6b6b6b;
        }

        #header .search-logo-cart .fa-shopping-cart:before {
            position: relative;
            top: 2px;
            margin-right: 5px;
            font-family: 'FontAwesome';
            font-size: 16px;
            line-height: 1;
        }

        #header .search-logo-cart .fa-shopping-cart .items {
            color: #959595;
        }

        #header .search-logo-cart .fa-shopping-cart:hover {
            background: #222222;
            color: #ffffff;
        }

            #header.white .search-logo-cart .fa-shopping-cart:hover {
                background: #f1f1f1;
                color: #000000;
            }

/* header navigation */
#header .navigation {
    display: block;
    overflow: visible;
    position: relative;
    height: 80px;
    text-align: center;
}

    #header .navigation li {
        display: inline-block;
        margin: 0 0 0 40px;
    }

        #header .navigation li:first-child {
            margin-left: 0;
        }

        #header .navigation li a {
            z-index: 999999;
            display: block;
            position: relative;
            color: #cccccc;
            text-transform: uppercase;
            line-height: 80px;
        }

            #header .navigation li:hover a:after, #header .navigation li.active a:after {
                content: '';
                position: absolute;
                bottom: 0;
                left: 0;
                width: 100%;
                height: 2px;
                background: #ffffff;
            }

            #header.white .navigation li a {
                color: #6b6b6b;
            }

            #header .navigation li a:hover, #header .navigation li.active a, #header .navigation li.has-sub:hover a {
                color: #ffffff;
            }

                #header.white .navigation li a:hover, #header.white .navigation li.active a {
                    color: #000000;
                }

        /* header navigation sub */
        #header .navigation li .sub {
            display: none;
            z-index: 99999;
            position: absolute;
            top: 0;/*80px*/
            left: 0;
            padding: 20px;
              height:250px;
            width: -moz-calc(100% - 40px);
            width: -webkit-calc(100% - 40px);
            width: calc(100% - 40px);
            background: #ffffff;
            border-top: 80px solid;
            border-bottom: 1px solid #f1f1f1;
            text-align: left;
        }

            #header .navigation li:hover .sub {
                display: block;
            }

            #header .navigation li .sub .float {
                float: left;
                margin: 0 0 0 20px;
                width: -moz-calc(25% - 15px);
                width: -webkit-calc(25% - 15px);
                width: calc(25% - 15px);
                height: 215px;
            }

                #header .navigation li .sub .float.has-table {
                    height: 194px;
                }

                #header .navigation li .sub .float:first-child {
                    margin-left: 0;
                }

                #header .navigation li .sub .float .block {
                    margin: 40px 0 0 0;
                }

                    #header .navigation li .sub .float .block:first-child {
                        margin-top: 0;
                    }

                    /* header navigation sub title */
                    #header .navigation li .sub .float .block .title {
                        display: block;
                        margin: 0 0 20px 0;
                        padding: 0 0 10px 0;
                        height: 12px;
                        border-bottom: 1px solid #f1f1f1;
                        text-transform: uppercase;
                        line-height: 1;
                    }

                    /* header navigation sub link */
                    #header .navigation li .sub .float .block .link {
                        display: block;
                        margin: 10px 0 0 0;
                        color: #acacac;
                        text-transform: none;
                        line-height: 1;
                    }

                        #header .navigation li .sub .float .block .link:before {
                            content: '\f105';
                            position: relative;
                            margin-right: 5px;
                            font-family: 'FontAwesome';
                            font-size: 12px;
                            line-height: 1;
                        }

                        #header .navigation li .sub .float .block .link:first-child {
                            margin-top: 0;
                        }

                        #header .navigation li .sub .float .block .link:hover {
                            color: #000000;
                        }

                    /* header navigation sub table */
                    #header .navigation li .sub .float .table {
                        display: table;
                        margin: 21px 0 0 0;
                        width: 100%;
                        height: 100%;
                        border: 1px dashed #f1f1f1;
                    }

                        #header .navigation li .sub .float .table .table-cell {
                            display: table-cell;
                            height: 100%;
                            text-align: center;
                            vertical-align: middle;
                        }

                            #header .navigation li .sub .float .table .table-cell h4 {
                                margin: 0 0 10px 0;
                                font-size: 14px;
                                text-transform: uppercase;
                                line-height: 1;
                            }

                                #header .navigation li .sub .float .table .table-cell h4:before {
                                    display: block;
                                    margin: 0 0 20px 0;
                                    font-family: 'FontAwesome';
                                    font-size: 48px;
                                }

                            #header .navigation li .sub .float .table .table-cell p {
                                color: #cec63d;
                                font-style: italic;
                                line-height: 1;
                            }

